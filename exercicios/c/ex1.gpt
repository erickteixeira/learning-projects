algoritmo sistema;

variáveis 
    A,B,C,D,E,F,x,y : real;
 fim-variáveis

 início
      imprima("Digite o número A : ");
      A :=leia();
      imprima("Digite o número B : ");
      B :=leia();
      imprima("Digite o número C : ");
      C :=leia();
      imprima("Digite o número D : ");
      D :=leia();
      imprima("Digite o número E : ");
      E :=leia();
      imprima("Digite o número F : ");
      F :=leia();
      y := ((F - D * C/A))/((-D * B)/(A + E));
      x := ((C - B *y)/A);
      imprima("A variavel y : ", y);
      imprima("A variavel x : ", x);
fim      

